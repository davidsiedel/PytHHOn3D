\newcommand{\Mesh}{\mathcal{T}_h}

\newcommand{\Rd}{\mathbb{R}^d}

\newcommand{\Rdd}{\mathbb{R}^{d \times d}}

\newcommand{\HMesh}{H^1(\Mesh;\mathbb{R}^d) = \big\{ \scal{u} \in L^2(\Omega;\mathbb{R}^{d}), \forall T \in \Mesh, \scal{u} \vert_T \in H^1(T;\mathbb{R}^{d}) \big\}}

\newcommand{\HConformal}{H^1(\Omega;\mathbb{R}^d) = \big\{ \scal{u} \in L^2(\Omega;\mathbb{R}^{d}), \nabla \scal{u} \in L^2(\Omega;\mathbb{R}^{d}) \big\}}

\newcommand{\HDivOmega}{H_{\Div}(\Omega;\mathbb{R}^d) = \big\{ \tens{P} \in L^2(\Omega;\mathbb{R}^{d \times d}), \nabla \cdot \tens{P} \in L^2(\Omega;\mathbb{R}^{d \times d}) \big\}}

\newcommand{\HDivMesh}{H_{\Div}(\mathcal{T}_h;\mathbb{R}^d) = \big\{ \tens{P} \in L^2(T;\mathbb{R}^{d \times d}), \forall T \in \mathcal{T}_h, \nabla \cdot \tens{P} \vert_T \in L^2(T;\mathbb{R}^{d \times d}) \big\}}

